@{
    ViewBag.Title = "Login";

}
@section Scripts {
    <script src="~/Scripts/App/Login.js"></script>
}
<div class="op-btns">
    <div class="col-md-6">
        <div class="page-title">Wizlearn Test Login</div>
    </div>
</div>
<div class="demopage">
    <div class="container1">
        <div class="panel panel-default row">
            <div class="panel panel-heading"></div>
            <div class="panel-container">
                <div class="panel panel-body tab-content-cls">
                    <div class="form-horizontal">
                        <div class="row">
                            <div class="col-md-6">
                                <label class="col-lg-4 control-label">Email</label>
                                <div class="col-md-8">
                                    <input type="text" id="email" name="email" class="form-control ">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label class="col-lg-4 control-label">Password</label>
                                <div class="col-md-8">
                                    <input type="password" id="password" name="password" class="form-control ">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                            </div>
                        </div>
                            <div class="row">
                                <div class="col-md-3">
                                </div>
                                <div class="col-md-1">
                                    <input type="button" value="Login" class="btn btn-default btn-default-style btn-login-data" />
                                </div>
                                <div class="col-md-1">
                                    <input type="button" value="SignUp" class="btn btn-default btn-default-style btn-signup-data" />
                                </div>
                                <div class="col-md-7">
                                </div>
                            </div>
                        </div>
                </div>
            </div>
        </div>
    </div>

</div>

