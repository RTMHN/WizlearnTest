@{
    ViewBag.Title = "SignUp";

}
@section Scripts {
    <script src="~/Scripts/App/Signup.js"></script>
}
<div class="op-btns">
    <div class="col-md-6">
        <div class="page-title">Wizlearn Test SignUp</div>
    </div>

</div>
<div class="demopage">
    <div class="container1">
        <div class="panel panel-default row">
            <div class="panel panel-heading"></div>
            <div class="panel-container">
                <div class="panel panel-body tab-content-cls">
                    <div class="form-horizontal">
                        <div class="row">
                            <div class="col-md-6">
                                <label class="col-lg-4 control-label">First Name</label>
                                <div class="col-md-8">
                                    <input type="text" id="fname" name="fname" class="form-control ">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label class="col-lg-4 control-label">Last Name</label>
                                <div class="col-md-8">
                                    <input type="text" id="lname" name="lname" class="form-control ">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label class="col-lg-4 control-label">Full Name</label>
                                <div class="col-md-8">
                                    <input type="text" id="fullname" name="fullname" class="form-control ">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label class="col-lg-4 control-label">Mobile</label>
                                <div class="col-md-8">
                                    <input type="text" id="mobile" name="mobile" class="form-control ">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label class="col-lg-4 control-label">Address</label>
                                <div class="col-md-8">
                                    <input type="text" id="address" name="address" class="form-control ">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label class="col-lg-4 control-label">Gender</label>
                                <div class="col-md-8">
                                    @Html.DropDownList("gender", new List<SelectListItem>(), "Select Type", new { @class = "form-control" })
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label class="col-lg-4 control-label">DOB</label>
                                <div class="col-md-8">
                                    <input type="text" id="dob" name="dob" class="form-control ">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label class="col-lg-4 control-label">Email</label>
                                <div class="col-md-8">
                                    <input type="text" id="email" name="email" class="form-control ">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label class="col-lg-4 control-label">Password</label>
                                <div class="col-md-8">
                                    <input type="password" id="password" name="password" class="form-control ">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label class="col-lg-4 control-label">Confirm Password</label>
                                <div class="col-md-8">
                                    <input type="password" id="cpass" name="cpass" class="form-control ">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <input type="button" value="SignUp" class="btn btn-default btn-default-style btn-signup-data" />

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>

